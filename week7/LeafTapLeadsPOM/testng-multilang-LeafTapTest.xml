<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="LangSuite">
	<test thread-count="2" name="EnglishTest" parallel="classes"
		verbose="5">
		<parameter name="language" value="english"></parameter>
		<classes>
			<class name="testcases.CreateLeadTest" />
			<class name="testcases.DuplicateLeadTest" />
			<class name="testcases.CreateContactTest" />
			<class name="testcases.DeleteLeadTest" />
			<class name="testcases.LoginTest" />
			<class name="testcases.MergeLeadTest" />
		</classes>
	</test> <!-- LangTest -->
	<test thread-count="2" name="FrenchTest" parallel="classes"
		verbose="5">
		<parameter name="language" value="french"></parameter>
		<classes>
			<class name="testcases.CreateLeadTest" />
			<class name="testcases.DuplicateLeadTest" />
			<class name="testcases.CreateContactTest" />
			<class name="testcases.LoginTest" />
		</classes>
	</test>
</suite> <!-- LangSuite -->
